pipeline {
    agent any
    
    stages{
        stage ("Clean-WorkSpace"){
	    steps {
	        deleteDir()
	    }
	}
	stage("upload") {
	    steps {
                def inputFile = input message: 'Upload file', parameters: [file(name: "$workspace/tags.txt")]
	    }
        }
        stage ("Git-Chekcout"){
            steps {
	        dir("${env.WORKSPACE}"){
		git credentialsId: 'git-hima', url: 'https://github.com/ehimshr/aws_ec2-auto_tags_xlsx_python.git'
		}
	    }
        }		
   }
}
