import groovy.json.*;
import org.apache.*;
import org.apache.commons.io.FileUtils;
import org.codehaus.groovy.modules.HTTPBuilder.*;
import hudson.security.AuthorizationMatrixProperty;
import jenkins.model.Jenkins;
import hudson.FilePath;
import hudson.model.ParametersAction;
import hudson.model.FileParameterValue;
import hudson.model.Executor;

pipeline {
    agent any
    
    stages{
        stage ("Clean-WorkSpace"){
	    steps {
	        deleteDir()
	    }
	}
	stage("upload") {
	    steps {
		    script {
                def inputFile = input message: 'Upload file', parameters: [file(name: "$workspace/tags.txt")]
				}
	    }
        }
        stage ("Git-Chekcout"){
            steps {
	        dir("${env.WORKSPACE}"){
		git credentialsId: 'git-hima', url: 'https://github.com/ehimshr/aws_ec2-auto_tags_xlsx_python.git'
		}
	    }
        }		
   }
}
